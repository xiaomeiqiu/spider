<script type="text/javascript">

function initProducts(){		
		$("#products").delegate("a","mouseenter", function(){            
            var a = $(this);
            a.attr("in","1");
            window.setTimeout(function(){
                if(a.attr("in") == "1"){
                    a.removeClass("leave").addClass("on");
                }                
            },90);
		}).delegate("a", "mouseleave", function(){            
			var a = $(this);
            a.attr("in","0");
            a.removeClass("on").addClass("leave");
		});
	}
    (function(){
		initProducts();
		var moreList = document.getElementById("moreProdList");
		$("#lnkShowOthers").click(function(e){			
			moreList.style.display="block";
			var moreH = moreList.clientHeight;
			moreList.style.height="0px";
			var tmpH = 0;
			var timer = window.setInterval(function(){
				tmpH += 10;
				if(tmpH >= moreH){
					tmpH = moreH;
					window.clearInterval(timer);
				}
				moreList.style.height= tmpH + "px";
				
			},1);
			
			document.getElementById("otherFbk").style.display = "block";
			document.getElementById("otherProd").style.display = "none";
		});
	})();
</script>
