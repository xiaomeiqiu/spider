!function(){	!function(){
		var helpNum = '3863428';
		var $ = require('common:widget/lib/jquery/jquery.js');
		var $helpNum = $('.doctor-help-num');
		if(helpNum < 10000){
			$helpNum.html(helpNum);
		} else if (helpNum < 100000000) {
			$helpNum.html((helpNum / 10000).toFixed(1) + '万');
		} else {
			$helpNum.html((helpNum / 100000000).toFixed(1) + '亿');
		}
	}()
