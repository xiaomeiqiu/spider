    // 手机端直接监听每条新闻并跳转
    if (device !== 'pc') {
      $(function() {
        var $banner = $('#banners');

        $banner.delegate('.feed_tegao', 'click', function(ev) {
          // 判断是否是点击了tag
          var $target = $(ev.target);
          if ($target.attr('href') &&
            /^\/.*/.test($target.attr('href'))) return;

          var $item = $(ev.currentTarget);
          var href = $item.attr('data-href');

          setTimeout(function() {
            window.location.href = href;
          }, 100)
        });
      });
    }

